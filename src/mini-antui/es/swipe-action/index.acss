.am-swipe{position:relative;overflow:hidden}.am-swipe-content{position:relative;transition:all 250ms}.am-swipe-right{position:absolute;top:0;bottom:0;display:flex;right:0}.am-swipe-btn{padding:0 14px;justify-content:center;align-items:center;display:flex;color:#fff}.am-swipe-btn-delete{background-color:#f4333c}.am-swipe-btn-edit{background-color:#108ee9}.am-swipe-movable-area{position:absolute}.am-swipe-movable-area .am-swipe-content{transition:none}.am-swiping .am-swipe-content{transition:none}